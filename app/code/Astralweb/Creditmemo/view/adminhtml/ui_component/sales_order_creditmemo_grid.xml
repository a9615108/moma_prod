        <!--class="Astralweb\AddColumnGridShipment\Ui\Component\Listing\Column\AccessActions"-->
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <columns name="sales_order_creditmemo_columns">
        <column name="product_custom"  class="Astralweb\Creditmemo\Ui\Component\Listing\Column\AccessActions">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="bodyTmpl" xsi:type="string">ui/grid/cells/html</item>
                    <item name="visible" xsi:type="boolean">false</item>
                    <item name="label" xsi:type="string" translate="true">Product</item>
                </item>
            </argument>
        </column>
        <column name="telephone_billing">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="visible" xsi:type="boolean">true</item>
                    <item name="label" xsi:type="string" translate="true">Telephone Billing</item>
                </item>
            </argument>
        </column>
        <column name="telephone_shipping">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="visible" xsi:type="boolean">true</item>
                    <item name="label" xsi:type="string" translate="true">Telephone Shipping</item>
                </item>
            </argument>
        </column>
    </columns>
</listing>

